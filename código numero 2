// Codigo n√∫mero 2
#include <iostream>

using namespace std;

int main() 
{
    int i = 1, j = 2; // se les da un valor entero a las variables i ,j
    
    if(i > j && j > i)// se aplica un condicional y si estas dos condiciones son verdaderas i incrementa su valor en 1
        i++;//  i no incremeta en 1  porq no cumple la condicion i>j,  i=1
        
    if(i > j || j > i) //se aplica un condicional y si alguna de estas dos condiciones son verdaderas o ambas , se incrementa el valor de j en 1
        j++;   // como si se cumple la condicion ya que j>i; j se incrementa en 1 osea j=3
    if(i | j)  // esta condicion no hace nada
        i++;// i incrementa  en 1, osea i=2
    if(i & j) // esta se cumple
        j++;// como la anterior condicion se cumple j se incrementa en 1, ya j=3+1=4
    
    cout<<i * j<<endl;// se muestra el resultado de la multiplicacion de i*j=2*4=8
    return 0;
}
